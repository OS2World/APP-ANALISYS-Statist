msgstr "  statist [ opções ] arquivo_dados\n"

#: ../src/statist.c:453
msgid "Options:\n"
msgstr "Opções:\n"

#: ../src/statist.c:454
msgid "--help, -h, -?  : print this help message and exit\n"
msgstr "--help, -h, -?  : imprime esta mensagem de ajuda e sai\n"

#: ../src/statist.c:455
msgid ""
"--silent, --quiet\n"
"                : don't print menu etc. (for batch/script usage)\n"
msgstr ""
"--silent, --quiet\n"
"                : não imprimi menu etc.; para uso de scripts (não "
"interativo)\n"

#: ../src/statist.c:457
msgid "--log           : write results to log file `statist.log'\n"
msgstr "--log           : escreve resultados em arquivo de log 'statist.log'\n"

#: ../src/statist.c:458
msgid "--nofile        : don't read a data file when starting the program\n"
msgstr ""
"--nofile        : não lê um arquivo de dados quando iniciando o programa\n"

#: ../src/statist.c:459
msgid "--nobell        : no beep at errors and warnings\n"
msgstr "--nobell        : não bipa durante erros e avisos\n"

#: ../src/statist.c:460
msgid ""
"--thist         : histogram as text graphic instead of gnuplot-graphic\n"
msgstr ""
"--thist         : histograma como texto gráfico no lugar de gráfico do "
"gnuplot\n"

#: ../src/statist.c:461
msgid "--noplot        : no gnuplot-graphic\n"
msgstr "--noplot        : sem gráficos do gnuplot\n"

#: ../src/statist.c:463
msgid "--color         : colorized output\n"
msgstr "--color         : saída colorida\n"

#: ../src/statist.c:465
msgid "--labels <file> : read column titles and value labels from file\n"
msgstr ""
"--labels <arq.> : extrai de um arquivo os rótulos de colunas e de valores\n"

#: ../src/statist.c:466
msgid "--header        : the file has column names in the first line\n"
msgstr "--header        : o arquivo possui nomes das colunas na 1ª linha\n"

#: ../src/statist.c:467
msgid "--noheader      : the file does not have column names\n"
msgstr "--noheader      : o arquivo não possui nomes das colunas\n"

#: ../src/statist.c:468
msgid "--sep <char>    : field separator character\n"
msgstr "--sep <caract.> : caractere separador de campos\n"

#: ../src/statist.c:469
msgid "--dec <char>    : decimal delimiter character (default: '.')\n"
msgstr "--dec <caract.> : caractere delimitador de decimais\n"

#: ../src/statist.c:470
msgid ""
"--na-string <string>\n"
"                : indicator of missing values (default: \"M\")\n"
msgstr ""
"--na-string <string>\n"
"                : indicador de valores omissos (default: \"M\")\n"

#: ../src/statist.c:472
msgid ""
"--xcols <conf_file> <orig_datafile> <new_data_file>\n"
"                : extract columns from a fixed width data file\n"
msgstr ""
"--xcols <arq_config> <arq_dados_orig> <novo_arq_dados>\n"
"                : extrai colunas de um arquivo de colunas de largura fixa\n"

#: ../src/statist.c:474
msgid ""
"--xsample <percentage> <database> <dest_file>\n"
"                : extract a sample of rows from a file\n"
msgstr ""
"--xsample <porcentagem> <arq_dados> <arq_destino>\n"
"                : extrai uma amostra das linhas de um arquivo\n"

#: ../src/statist.c:476
msgid ""
"--bernhard      : special output changes from Bernhard, i.e.:\n"
"                  - table output at Miscellaneous/Standard deviation\n"
"                  - if --noplot defined no text histogram at\n"
"                       Miscellaneous/Standard deviation\n"
msgstr ""
"--bernhard      : mudanças especiais no output de Bernhard, i.e.:\n"
"                   - produz tabela em diversos/desvio padrão\n"
"                   - se --noplot estiver definido, não haverá histograma em\n"
"                     modo texto em Diversos | Desvio padrão\n"

